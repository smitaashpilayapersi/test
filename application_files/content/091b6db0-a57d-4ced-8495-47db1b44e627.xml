<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>AB_createAppInstanceChartSeries</name>
        <uuid>091b6db0-a57d-4ced-8495-47db1b44e627</uuid>
        <description></description>
        <parentUuid>_a-0000dc7e-760f-8000-927a-010000010000_199998</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!arrAppInstanceData: index(
    ri!arrcdtData,
    wherecontains(
      ri!intAppId,
      tointeger(
        ri!arrcdtData.AppId
      )
    ),
    {}
  ),
  local!data: #&quot;SYSTEM_SYSRULES_applyComponents&quot;(
    function: tointeger(
      _
    ),
    array: #&quot;SYSTEM_SYSRULES_applyComponents&quot;(
      function: wherecontains(
        _,
        todate(
          local!arrAppInstanceData.CreationDate
        )
      ),
      array: ri!arrdtDateRange
    )
  ),
  #&quot;SYSTEM_SYSRULES_chartSeries&quot;(
    label: ri!txtAppName,
    data: apply(
      index(
        local!arrAppInstanceData,
        _,
        &quot;InstanceCount&quot;,
        null()
      ),
      local!data
    )
  )
)</definition>
        <namedTypedValue>
            <name>arrcdtData</name>
