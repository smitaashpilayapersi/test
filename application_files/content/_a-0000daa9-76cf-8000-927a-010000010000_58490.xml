<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>PFS_UTIL_ucDocumentImageEach</name>
        <uuid>_a-0000daa9-76cf-8000-927a-010000010000_58490</uuid>
        <description>Returns a Document Image for use by the image field component</description>
        <parentUuid>_a-0000da76-7afb-8000-6ee0-010000010000_1389</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>= #&quot;SYSTEM_SYSRULES_documentImage&quot;(
  document: ri!document,
  caption: document(
    ri!document,
    &quot;name&quot;
  )
)</definition>
        <namedTypedValue>
            <name>document</name>
            <type>
                <name>CollaborationDocument</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
