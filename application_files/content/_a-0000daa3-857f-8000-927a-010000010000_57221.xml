        <uuid>_a-0000daa3-857f-8000-927a-010000010000_57221</uuid>
        <description>This rule takes the inputs element and array. It returns the index that indicate the position of the values within the array</description>
        <parentUuid>_a-0000da76-7afb-8000-6ee0-010000010000_1389</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>= wherecontains(
  ri!element,
  ri!array
)</definition>
        <namedTypedValue>
            <name>element</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
