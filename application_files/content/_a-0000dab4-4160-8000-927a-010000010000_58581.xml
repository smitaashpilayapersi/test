<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>PFS_UTIL_ucMultiFileUploadRenderField</name>
        <uuid>_a-0000dab4-4160-8000-927a-010000010000_58581</uuid>
        <description>Returns a file upload component populated with the value at the given index.</description>
        <parentUuid>_a-0000da76-7afb-8000-6ee0-010000010000_1389</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>= with(
  local!paddedArray: append(
    ri!files,
    null
  ),
  #&quot;SYSTEM_SYSRULES_fileUploadField&quot;(
    label: if(
      ri!index = 1,
      ri!label,
      &quot;&quot;
    ),
    instructions: &quot;To upload images first use  ' Upload Images ' button, before saving the component &quot;,
    target: ri!target,
    value: local!paddedArray[ri!index],
    saveInto: {
      a!save(
        ri!files,
        #&quot;_a-0000dab4-4160-8000-927a-010000010000_58582&quot;(
          ri!files,
          ri!index,
          save!value
        )
      )
    }
  )
)</definition>
        <namedTypedValue>
            <name>label</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>files</name>
            <type>
                <name>CollaborationDocument?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>target</name>
            <type>
                <name>CollaborationDocumentOrFolder</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>index</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
        <role allowForAll="false" inherit="true" name="readers">
            <users/>
            <groups/>
        </role>
        <role allowForAll="false" inherit="true" name="authors">
            <users/>
            <groups/>
        </role>
        <role allowForAll="false" inherit="true" name="administrators">
            <users/>
            <groups/>
        </role>
        <role allowForAll="false" inherit="false" name="denyReaders">
            <users/>
            <groups/>
        </role>
        <role allowForAll="false" inherit="false" name="denyAuthors">
            <users/>
            <groups/>
        </role>
        <role allowForAll="false" inherit="false" name="denyAdministrators">
            <users/>
            <groups/>
        </role>
    </roleMap>
</contentHaul>
