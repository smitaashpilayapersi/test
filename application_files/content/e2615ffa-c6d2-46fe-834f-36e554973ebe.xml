<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>AB_createAppInstanceStatusChartSeries</name>
        <uuid>e2615ffa-c6d2-46fe-834f-36e554973ebe</uuid>
        <description></description>
        <parentUuid>_a-0000dc7e-760f-8000-927a-010000010000_199998</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!arrintIndices: wherecontains(
    ri!txtStatus,
    ri!arrtxtApplicationInstanceStatus
  ),
  local!arrintAppInstanceIds: index(
    ri!arrintAppInstanceIds,
    local!arrintIndices,
    {}
  ),
  #&quot;SYSTEM_SYSRULES_chartSeries&quot;(
    label: ri!txtStatus,
    data: count(
      local!arrintIndices
    ),
    links: #&quot;SYSTEM_SYSRULES_dynamicLink&quot;(
      value: local!arrintAppInstanceIds,
      saveInto:
      {
        a!save(
          ri!arrintSelectedAppInstanceId,
          save!value
        ),
        a!save(
          ri!SelectedStatus,
          ri!txtStatus
        )
        
      }
    )
  )
)</definition>
        <namedTypedValue>
            <name>txtStatus</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>arrtxtApplicationInstanceStatus</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>arrintAppInstanceIds</name>
            <type>
                <name>Integer?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>arrintSelectedAppInstanceId</name>
            <type>
                <name>Integer?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>SelectedStatus</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>interface</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
